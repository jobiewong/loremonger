{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "opener:default",
    "stronghold:default",
    "dialog:default",
    "fs:default",
    "fs:allow-create",
    "fs:allow-write-file",
    "fs:allow-write-text-file",
    "sql:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close",
    "core:path:default"
  ]
}
